<?xml version="1.0" encoding="UTF-8"?>
<!-- test that an expression that cannot be interpreted as a boolean
is treated as false -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  version="1.0" initial="s0">

  <state id="s0">
    <transition cond="1 = 1" target="fail" />
    <transition target="pass" />
  </state>

  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>


</scxml>
