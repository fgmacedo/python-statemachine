<?xml version="1.0" encoding="UTF-8"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  initial="s0" version="1.0" datamodel="python">


  <state id="s0">
    <onentry>
      <send type="http://www.w3.org/TR/scxml/#SCXMLEventProcessor" event="event"
        target="#_scxml_foo" />
      <raise event="foo" />
    </onentry>
    <transition event="error.communication" target="pass" />
    <transition event="*" target="fail" />
  </state>


  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>

</scxml>
