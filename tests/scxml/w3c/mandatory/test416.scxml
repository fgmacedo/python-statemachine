<?xml version="1.0" encoding="UTF-8"?><!-- test that the done.state.id gets generated when we enter the
final state of a compound state -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  version="1.0" initial="s1" datamodel="python">

  <state id="s1" initial="s11">
    <onentry>
      <send event="timeout" delay="1s" />
    </onentry>
    <transition event="timeout" target="fail" />

    <state id="s11" initial="s111">
      <transition event="done.state.s11" target="pass" />
      <state id="s111">
        <transition target="s11final" />
      </state>
      <final id="s11final" />
    </state>
  </state>


  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>

</scxml>
