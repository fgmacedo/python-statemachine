<?xml version="1.0" encoding="UTF-8"?><!-- test that markup can be specified both by 'src' and by <content>  -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  initial="s0" version="1.0" datamodel="python">


  <state id="s0" initial="s01">
    <onentry>
      <send event="timeout" delay="2s" />
    </onentry>
    <transition event="timeout" target="fail" />

    <state id="s01">
      <invoke type="http://www.w3.org/TR/scxml/" src="file:test239sub1.scxml" />
      <transition event="done.invoke" target="s02" />
    </state>

    <state id="s02">
      <invoke type="http://www.w3.org/TR/scxml/">
        <!-- identical to test239sub1.scxml.  -->
        <content>
          <scxml version="1.0" initial="final" datamodel="python">
            <final id="final" />
          </scxml>
        </content>
      </invoke>

      <transition event="done.invoke" target="pass" />
    </state>

  </state>

  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>
</scxml>
