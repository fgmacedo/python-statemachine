<?xml version="1.0" encoding="UTF-8"?>
<scxml datamodel="python" initial="s0" name="ScxmlTest529" version="1.0"
	xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"><!--simple
	test that children workn with <content>-->
	<state id="s0" initial="s01">
		<transition cond="_event.data == 21" event="done.state.s0" target="pass" />
		<transition event="done.state.s0" target="fail" />
		<state id="s01">
			<transition target="s02" />
		</state>
		<final id="s02">
			<donedata>
				<content>21</content>
			</donedata>
		</final>
	</state>
	<final id="pass">
		<onentry>
			<log expr="'pass'" label="Outcome" />
		</onentry>
	</final>
	<final id="fail">
		<onentry>
			<log expr="'fail'" label="Outcome" />
		</onentry>
	</final>
</scxml>
