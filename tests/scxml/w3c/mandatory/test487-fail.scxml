<?xml version="1.0" encoding="UTF-8"?><!-- test illegal assignment.  error.execution should be raised.  -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  initial="s0" datamodel="python" version="1.0">
  <datamodel>
    <data id="Var1" expr="1 = 1" />
  </datamodel>

  <state id="s0">
    <onentry>
      <raise event="event" />
    </onentry>

    <transition event="error.execution" target="pass" />
    <transition event="*" target="fail" />
  </state>

  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>
</scxml>
