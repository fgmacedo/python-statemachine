<?xml version="1.0" encoding="UTF-8"?><!-- make sure sendid is blank in a non-error event -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  version="1.0" initial="s0" datamodel="python" name="machineName">

  <state id="s0">
    <onentry>
      <send event="foo" />
    </onentry>
    <transition event="foo" cond="not _event.sendid" target="pass" />
    <transition event="*" target="fail" />
  </state>


  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>

</scxml>
