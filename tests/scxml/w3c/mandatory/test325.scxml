<?xml version="1.0" encoding="UTF-8"?><!-- test that _ioprocessors is bound at startup.  I'm not sure how to
test for a set value -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
   initial="s0" datamodel="python" version="1.0" name="machineName">
   <datamodel>
      <data id="Var1" expr="_ioprocessors" />
   </datamodel>


   <state id="s0">
      <transition cond="Var1" target="pass" />
      <transition target="fail" />
   </state>


   <final id="pass">
      <onentry>
         <log label="Outcome" expr="'pass'" />
      </onentry>
   </final>
   <final id="fail">
      <onentry>
         <log label="Outcome" expr="'fail'" />
      </onentry>
   </final>

</scxml>
