<?xml version="1.0" encoding="UTF-8"?><!-- test that invokeid is blank in an event that wasn't returned
from an invoked process -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  initial="s0" version="1.0" datamodel="python" name="machineName">

  <state id="s0">
    <onentry>
      <raise event="foo" />
    </onentry>
    <transition event="foo" cond="not _event.invokeid" target="pass" />
    <transition event="*" target="fail" />
  </state>


  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>

</scxml>
