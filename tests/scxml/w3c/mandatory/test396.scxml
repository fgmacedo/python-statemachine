<?xml version="1.0" encoding="UTF-8"?><!-- test that the value in _event.name matches the event name used
to match against transitions  -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  datamodel="python" version="1.0">

  <state id="s0">
    <onentry>
      <raise event="foo" />
    </onentry>


    <transition event="foo" cond="_event.name == 'foo'" target="pass" />
    <transition event="foo" target="fail" />

  </state>

  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>
</scxml>
