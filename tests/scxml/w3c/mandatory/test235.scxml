<?xml version="1.0" encoding="UTF-8"?><!-- test that done.invoke.id event has the right id.  the invoked
child terminates immediately
and should generate done.invoke.foo   -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  initial="s0" version="1.0" datamodel="python">


  <state id="s0">
    <onentry>
      <send event="timeout" delay="2s" />
    </onentry>
    <invoke type="http://www.w3.org/TR/scxml/" id="foo">
      <content>
        <scxml initial="subFinal" version="1.0" datamodel="python">
          <final id="subFinal" />
        </scxml>
      </content>
    </invoke>

    <transition event="done.invoke.foo" target="pass" />
    <transition event="*" target="fail" />
  </state>

  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>
</scxml>
