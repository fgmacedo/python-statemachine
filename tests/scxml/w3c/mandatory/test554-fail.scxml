<?xml version="1.0" encoding="UTF-8"?><!-- test that if the evaluation of <invoke>'s args causes an error,
the
invocation is cancelled.  In this test, that means that we don't get done.invoke
before the timer goes off.  -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance"
  initial="s0" version="1.0" datamodel="python">


  <state id="s0">
    <onentry>
      <send event="timer" delay="2s" />
    </onentry>

    <!-- namelist references an undeclared variable -->
    <invoke type="http://www.w3.org/TR/scxml/" namelist="Var2">
      <content>
        <scxml initial="subFinal" version="1.0" datamodel="python">
          <final id="subFinal" />
        </scxml>
      </content>
    </invoke>
    <transition event="timer" target="pass" />
    <transition event="done.invoke" target="fail" />
  </state>

  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'" />
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'" />
    </onentry>
  </final>
</scxml>
